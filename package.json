{
	"name": "central-base-mssql",
	"version": "1.0.0",
	"description": "a seed project for creating http service using express written in typescript",
	"main": "app.js",
	"scripts": {
		"test": "jest --no-cache",
		"start": "npm run dev",
		"prod": "cross-env NODE_ENV=production webpack --watch",
		"dev": "webpack --watch",
		"build": "npm run build:dev",
		"build:dev": "webpack --config ./webpack.config.js --progress --profile --color --display-error-details --display-cached && tsc",
		"build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --profile --color --display-error-details --display-cached --bail && tsc",
		"clean": "npm cache clear && rimraf -- dist"
	},
	"keywords": [],
	"author": "Rodrigo Sombrio <rodrigo.sombrio@totvs.com.br> (http://www.totvs.com)",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.18.3",
		"compression": "^1.7.2",
		"config": "^1.31.0",
		"cookie-parser": "^1.4.3",
		"cors": "^2.8.5",
		"debug": "^3.1.0",
		"del": "^3.0.0",
		"errorhandler": "^1.5.0",
		"express": "^4.16.3",
		"express-session": "^1.15.6",
		"express-status-monitor": "^1.2.3",
		"helmet": "^3.12.1",
		"joi": "^13.4.0",
		"lodash": "^4.17.10",
		"lokijs": "^1.5.5",
		"method-override": "^2.3.10",
		"mongoose": "^5.1.5",
		"morgan": "^1.9.0",
		"mssql": "^4.3.2",
		"multer": "^1.3.0",
		"node-cache": "^4.2.0",
		"node-schedule": "^1.3.2",
		"reflect-metadata": "^0.1.13",
		"request-promise": "^4.2.4",
		"rxjs": "^6.2.1",
		"typeorm": "^0.2.14",
		"wicket": "^1.3.3",
		"winston": "^3.0.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.4",
		"@types/mssql": "^4.0.12",
		"@types/node-cache": "^4.1.3",
		"@types/node-schedule": "^1.2.3",
		"@types/request-promise": "^4.1.42",
		"@reactivex/rxjs": "^6.2.1",
		"@types/body-parser": "1.17.0",
		"@types/compression": "^0.0.36",
		"@types/config": "0.0.34",
