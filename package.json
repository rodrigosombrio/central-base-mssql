		"webpack-merge": "^4.1.2"
	},
	"jest": {
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"testRegex": "\\.test\\.ts",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
