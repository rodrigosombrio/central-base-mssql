		"@types/express": "^4.16.1",
		"@types/express-session": "^1.15.10",
		"@types/helmet": "^0.0.38",
		"@types/jest": "^23.0.2",
		"@types/lodash": "^4.14.109",
		"@types/lokijs": "^1.5.2",
		"@types/method-override": "^0.0.31",
		"@types/morgan": "^1.7.35",
		"@types/multer": "^1.3.6",
		"@types/node": "^10.3.2",
		"@types/supertest": "^2.0.4",
		"@types/webpack": "^4.4.0",
		"cross-env": "^5.1.6",
		"jest": "^24.1.0",
